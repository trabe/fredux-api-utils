{
  "name": "fredux-api-utils",
  "version": "2.0.1",
  "description": "fabulous redux api utils",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha ./test/*-test.js -r jsdom-global/register --compilers js:babel-register --recursive",
    "test:watch": "mocha ./test/*-test.js -r jsdom-global/register --compilers js:babel-register --recursive --watch",
    "clean": "rm -rf dist/*",
    "lint": "eslint ./src ./test",
    "lint:fix": "eslint ./src ./test --fix",
    "precompile": "npm run clean",
    "compile": "babel -d dist/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Trabe",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.6.0",
    "eslint-config-trabe": "^0.0.5",
    "eslint-plugin-prettier": "^2.0.1",
    "expect": "^1.20.2",
    "fetch-mock": "^5.12.1",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "11.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^3.5.0",
    "prettier": "^1.5.3",
    "sinon": "^1.17.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "prettier"
    ],
    "extends": [
      "trabe",
      "prettier",
      "prettier/react"
    ]
  }
}
